{
    "name": "tightcnc",
    "version": "0.3.0-alpha0",
    "description": "Backend-centric job-oriented server for hobby cnc controllers",
    "keywords": [
        "cnc",
        "gcode",
        "tinyg",
        "g2core"
    ],
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "server:debug": "ts-node --inspect-brk ./src/server/server.ts",
        "server": "ts-node ./src/server/server.ts",
        "consoleui": "ts-node ./src/consoleui/consoleui.ts",
        "server:js": "node ./dist/src/server/server.js",
        "consoleui:js": "node ./dist/src/consoleui/consoleui.js",
        "cli:js": "node ./bin/cli.js"
    },
    "bin": {
        "tightcnc-server": "./bin/tightcnc-server.js",
        "tightcnc-consoleui": "./bin/tightcnc-consoleui.js",
        "tightcnc": "./bin/cli.js"
    },
    "main": "src/index.js",
    "dependencies": {
        "@serialport/binding-abstract": "^9.0.7",
        "@serialport/stream": "^9.0.7",
        "axios": "^0.21.1",
        "blessed": "^0.1.81",
        "common-schema": "^3.0.0",
        "crisphooks": "^2.0.1",
        "cross": "^1.0.0",
        "express": "^4.17.1",
        "kd-tree-javascript": "^1.0.3",
        "littleconf": "^1.0.1",
        "mkdirp": "^1.0.4",
        "moment": "^2.29.1",
        "objtools": "^3.0.0",
        "pasync": "^2.0.2",
        "request": "^2.88.0",
        "serialport": "^9.1.0",
        "stable": "^0.1.8",
        "xerror": "^2.0.0",
        "yaar": "^3.0.2",
        "yargs": "^17.0.1",
        "zstreams": "^5.0.0"
    },
    "devDependencies": {
        "@types/blessed": "^0.1.17",
        "@types/express": "^4.17.12",
        "@types/kd-tree-javascript": "^1.0.0",
        "@types/mkdirp": "^1.0.1",
        "@types/node": "^15.12.1",
        "@types/objtools": "^2.0.0",
        "@types/serialport": "^8.0.1",
        "markdown-toc": "^1.2.0",
        "ts-migrate": "^0.1.19",
        "ts-node": "^10.0.0",
        "tslib": "^2.2.0",
        "typescript": "^4.3.2"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/crispy1989/tightcnc.git"
    }
}
