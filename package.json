{
    "name": "tightcnc",
    "version": "0.3.0-alpha1",
    "description": "Backend-centric job-oriented server for hobby cnc controllers",
    "keywords": [
        "cnc",
        "grbl",
        "gcode",
        "tinyg",
        "g2core",
        "typescript"
    ],
    "scripts": {
        "build": "webpack --progress --mode production",
        "build:dev": "webpack --mode development --stats-error-details",
        "watch": "webpack --mode development --watch --stats-error-details",
        "server:debug": "ts-node --inspect-brk ./src/server/server.ts",
        "server": "ts-node ./src/server/server.ts",
        "consoleui": "ts-node ./src/consoleui/consoleui.ts",
        "server:bin": "node ./bin/tightcnc-server.js",
        "consoleui:bin": "node ./bin/tightcnc-consoleui.js",
        "cli:bin": "node ./bin/cli.js",
        "server:js": "node ./dist/server/server.js",
        "consoleui:js": "node ./dist/consoleui/consoleui.js"
    },
    "bin": {
        "tightcnc": "./bin/cli.js",
        "tightcnc-consoleui": "./bin/tightcnc-consoleui.js",
        "tightcnc-server": "./bin/tightcnc-server.js"
    },
    "main": "dist/server/server.js",
    "types": "types/src/index.d.ts",
    "dependencies": {
        "@serialport/binding-abstract": "^9.0.7",
        "@serialport/stream": "^9.0.7",
        "axios": "^0.21.1",
        "blessed": "^0.1.81",
        "common-schema": "^3.0.0",
        "cors": "^2.8.5",
        "crisphooks": "^2.0.1",
        "cross": "^1.0.0",
        "express": "^4.17.1",
        "json-rpc-2.0": "^0.2.16",
        "kd-tree-javascript": "^1.0.3",
        "littleconf": "^1.0.1",
        "mkdirp": "^1.0.4",
        "moment": "^2.29.1",
        "objtools": "^3.0.0",
        "pasync": "^2.0.2",
        "request": "^2.88.2",
        "serialport": "^9.1.0",
        "stable": "^0.1.8",
        "term.js": "^0.0.7",
        "xerror": "^2.0.0",
        "xhr2": "^0.2.1",
        "yargs": "^17.0.1",
        "zstreams": "^5.0.0"
    },
    "devDependencies": {
        "@types/blessed": "^0.1.17",
        "@types/cors": "^2",
        "@types/express": "^4.17.12",
        "@types/kd-tree-javascript": "^1.0.0",
        "@types/mkdirp": "^1.0.1",
        "@types/node": "^15.12.2",
        "@types/objtools": "^2.0.0",
        "@types/serialport": "^8.0.1",
        "@types/terser-webpack-plugin": "^5.0.3",
        "@types/webpack-manifest-plugin": "^3.0.4",
        "markdown-toc": "^1.2.0",
        "node-polyfill-webpack-plugin": "^1.1.2",
        "terser-webpack-plugin": "^5.1.3",
        "ts-loader": "^9.2.3",
        "ts-migrate": "^0.1.19",
        "ts-node": "^10.0.0",
        "tslib": "^2.2.0",
        "typescript": "^4.3.2",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.2",
        "webpack-manifest-plugin": "^3.1.1",
        "webpack-node-externals": "^3.0.0"
    },
    "license": "Apache-2.0",
    "_type": "commonjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/crispy1989/tightcnc.git"
    }
}
